#include <iostream>

using namespace std;

int main()
{
int c,p;
int i;
long double factorial = 1.0;
cout<<"Enter the number of people\n";
cin>> p;
cout<<"Enter the number of chairs\n";
cin>> c;

cout<<"The number of of arrangements are;\n";
for ( i = 1; i <= p; i++)
{
   cout<<"\n";
  factorial *= i;

}



    return 0;
}
