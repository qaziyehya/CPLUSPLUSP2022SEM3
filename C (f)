#include <iostream>
using namespace std;

int main()
{

    int a, b, c;

    cout << "Enter the age of Ram, Shyam & Ajay \n";

    
    cin >> a >> b >> c;

    

    if (a < b && a < c)
    {
        cout << "Ram is the youngest " a;

        
    }
    else if (b < a && b < c)
    {
        cout << "Shyam is the youngest ";
    }
    else
    {
        cout << "Ajay is the youngest" ;
    }

    return 0;
}
